#!/bin/sh
#
# Copyright (C) 2013-2015 OpenWrt.org
#

. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case "$board" in
lorawan_gateway_ethernet|\
lorawan_gateway_wifi|\
video_stream_ethernet|\
video_stream_wifi|\
flexcan_ethernet|\
audio_stream_ethernet)
	ucidef_set_interface_lan 'eth0'
	;;
esac

board_config_flush

exit 0
